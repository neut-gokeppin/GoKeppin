<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/Layout2"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="horizontal" >

    <!-- 左領域 -->
    <LinearLayout
        android:id="@+id/LinearLayout001"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:orientation="vertical"
        android:padding="0dp" >

        <!-- 情報 -->
        <LinearLayout
            android:id="@+id/LinearLayout001_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="5"
            android:orientation="horizontal"
            android:padding="0dp" >

            <!-- 車両情報 -->
            <LinearLayout
                android:id="@+id/LinearLayout001_11"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:orientation="vertical"
                android:padding="0dp" >

                <!-- 撮影日時 -->
                <LinearLayout
                    android:id="@+id/LinearLayout001_111"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="0dp" >

                    <TextView
                        android:id="@+id/TextView001_11"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="@string/shotimageFileDateLabel"
                        android:textSize="30sp"
                        android:typeface="monospace" />

                    <TextView
                        android:id="@+id/shotimageFileDate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="撮影日時"
                        android:textSize="40sp"
                        android:typeface="monospace" />

                </LinearLayout>

                <!-- 車種名、ボデーNO -->
                <LinearLayout
                    android:id="@+id/LinearLayout001_112"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="0dp" >

                    <TextView
                        android:id="@+id/TextView001_21"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="@string/vehicleNameLabel"
                        android:textSize="30sp"
                        android:typeface="monospace" />

                    <TextView
                        android:id="@+id/vehicleName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="車種"
                        android:textSize="40sp"
                        android:typeface="monospace" />

                    <TextView
                        android:id="@+id/TextView001_22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="　"
                        android:textSize="30sp"
                        android:typeface="monospace" />

                    <TextView
                        android:id="@+id/TextView001_23"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="@string/bodyNoLabel"
                        android:textSize="30sp"
                        android:typeface="monospace" />

                    <TextView
                        android:id="@+id/bodyNo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="ボデーNO"
                        android:textSize="40sp"
                        android:typeface="monospace" />

                    </LinearLayout>

                <!-- 項目 -->
                <LinearLayout
                    android:id="@+id/LinearLayout001_113"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="0dp" >

                    <TextView
                        android:id="@+id/TextView001_31"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="@string/itemNameLabel"
                        android:textSize="30sp"
                        android:typeface="monospace" />

                    <TextView
                        android:id="@+id/itemName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="項目"
                        android:textSize="40sp"
                        android:typeface="monospace" />

                </LinearLayout>

            </LinearLayout>

            <!-- タクトタイム -->
            <LinearLayout
                android:id="@+id/LinearLayout001_12"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:padding="0dp" >

                <LinearLayout
                    android:id="@+id/LinearLayout001_121"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:padding="0dp" >

                    <TextView
                        android:id="@+id/tacttimeLabel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="@string/tacttimeLabel"
                        android:textSize="30sp"
                        android:typeface="monospace" />

                    <TextView
                        android:id="@+id/tacttime"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="タクトタイム"
                        android:textSize="80sp"
                        android:typeface="monospace" />

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <!-- 画像 -->
        <LinearLayout
            android:id="@+id/LinearLayout001_2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:padding="0dp" >

            <ImageView
                android:id="@+id/shotimage"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="fitCenter"
                android:adjustViewBounds="true"
                android:background="@drawable/btn_default_normal" />

            <TextView
                android:id="@+id/shotimageMsg"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="メッセージ"
                android:textSize="25sp"
                android:typeface="monospace" />

        </LinearLayout>

    </LinearLayout>

    <!-- 右領域 -->
    <LinearLayout
        android:id="@+id/LinearLayout002"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="vertical"
        android:padding="0dp" >

        <!-- 上部 -->
        <LinearLayout
            android:id="@+id/LinearLayout01"
            android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="0dp" >

            <LinearLayout
                android:id="@+id/LinearLayout06"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:padding="0dp" >

                <TextView
                    android:id="@+id/KS_TV_GROUPNAME"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:lines="2"
                    android:maxWidth="90dp"
                    android:padding="0dp"
                    android:text="&lt;&lt;グループ>>"
                    android:textSize="12sp" >

                </TextView>

                <TextView
                    android:id="@+id/KS_TV_KENSAJUN"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="4dp"
                    android:padding="0dp"
                    android:text="No.999"
                    android:textSize="12sp" >

                </TextView>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout04"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_weight="0"
                android:gravity="right"
                android:padding="0dp" >

                <Button
                    android:id="@+id/Menu_Button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="0dp"
                    android:soundEffectsEnabled="true"
                    android:text="@string/Menu_Button"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    android:width="60sp" >
                </Button>

                <Button
                    android:id="@+id/Mae_Button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:padding="0dp"
                    android:soundEffectsEnabled="true"
                    android:text="@string/Mae_Button"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    android:width="60sp" >

                </Button>

                <Button
                    android:id="@+id/Tsugi_Button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="0dp"
                    android:text="@string/Tsugi_Button"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    android:width="60sp" >
                </Button>
            </LinearLayout>
        </LinearLayout>

        <!-- 検査項目名称 -->
        <LinearLayout
            android:id="@+id/LinearLayout07"
            android:layout_width="fill_parent"
	        android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/KS_TV_KENSAITEM"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="検査項目名称"
                android:textSize="16sp"
                android:typeface="monospace" >
            </TextView>
        </LinearLayout>

        <!-- 画像１ -->
        <FrameLayout
            android:id="@+id/FrameLayout01"
            android:layout_width="fill_parent"
	        android:layout_height="175dp"
            android:layout_weight="4" >

            <ImageButton
                android:id="@+id/ImageButton01"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:maxHeight="300dp"
                android:minHeight="300dp"
                android:scaleType="fitCenter"
                android:src="@drawable/sample_1" >
            </ImageButton>

            <TextView
                android:id="@+id/tvImageKensaNaiyo1"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:gravity="center"
                android:typeface="monospace" />

            <TextView
                android:id="@+id/TextView01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/back"
                android:paddingRight="2dp"
                android:text="@+id/TextView01"
                android:textColor="@color/BLACK"
                android:textSize="20sp"
                android:textStyle="bold" >

            </TextView>
        </FrameLayout>

        <!-- 画像２ -->
        <FrameLayout
            android:id="@+id/FrameLayout02"
            android:layout_width="fill_parent"
	        android:layout_height="175dp"
            android:layout_weight="4" >

            <ImageButton
                android:id="@+id/ImageButton02"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:maxHeight="300dp"
                android:minHeight="300dp"
                android:scaleType="fitCenter"
                android:src="@drawable/sample_1" >
            </ImageButton>

            <TextView
                android:id="@+id/tvImageKensaNaiyo2"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:gravity="center"
                android:typeface="monospace" />

            <TextView
                android:id="@+id/TextView02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/back"
                android:paddingRight="2dp"
                android:text="@+id/TextView02"
                android:textColor="@color/BLACK"
                android:textSize="20sp"
                android:textStyle="bold" >

            </TextView>
        </FrameLayout>

        <!-- 他 -->
        <LinearLayout
            android:id="@+id/LinearLayout08"
            android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
            android:gravity="top" >

            <Button
                android:id="@+id/OtherButton"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="@string/other"
                android:textSize="20sp"
                android:textStyle="bold" >
            </Button>
        </LinearLayout>

        <!-- 下部 -->
        <LinearLayout
            android:id="@+id/LinearLayout11"
            android:layout_width="match_parent"
	        android:layout_height="wrap_content"
            android:gravity="bottom"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/KS_TV_BODYNO"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:gravity="bottom"
                android:text="12345"
                android:textSize="18sp" />

        </LinearLayout>

    </LinearLayout>

</LinearLayout>
